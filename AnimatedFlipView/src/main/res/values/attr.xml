<?xml version="1.0" encoding="utf-8"?>
<resources>
<declare-styleable name="AnimatedFlipView">
    <attr name="frontFlipDuration" format="integer" />
    <attr name="backFlipDuration" format="integer" />
    <attr name="flipDirection">
        <enum name="LEFT_TO_RIGHT" value="0" />
        <enum name="RIGHT_TO_LEFT" value="1" />
        <enum name="TOP_TO_DOWN" value="2" />
        <enum name="DOWN_TO_TOP" value="3" />
    </attr>
</declare-styleable>

</resources>